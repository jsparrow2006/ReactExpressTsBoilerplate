{
  "name": "NodeReactBoilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "_moduleAliases": {
    "controllers": "build/controllers/",
    "helpers": "build/helpers/",
    "libs": "build/libs/",
    "middlewares": "build/middlewares/",
    "models": "./build/models/",
    "routes": "build/routes/",
    "types": "build/types/",
    "config": "build/config/",
    "db": "build/db/"
  },
  "scripts": {
    "prestart": "source $HOME/.nvm/nvm.sh; nvm use; nvm --version",
    "ts:watch": "tsc -w",
    "node:dev": "cross-env NODE_ENV=development nodemon",
    "start": "concurrently \"yarn ts:watch\" \"yarn node:dev\"",
    "build": "yarn install --modules-folder ../build/node_modules && tsc --build build-tsconfig.json",
    "install-dependency": "yarn prestart; echo '\\033[34mInstall client dependency:\\033[m\\n' && yarn"
  },
  "author": "I.Gaichuk",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "connect-form": "^0.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.2",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "multiparty": "^4.2.2",
    "mysql2": "^1.7.0",
    "node-fetch": "^2.6.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "safe-buffer": "^5.2.0",
    "sequelize": "^6.25.2",
    "session-file-store": "^1.3.1",
    "sqlite3": "^5.1.6",
    "tedious": "^6.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/express-formidable": "^1.2.0",
    "@types/node": "^18.18.6",
    "@types/passport": "^1.0.11",
    "@types/sequelize": "^4.28.14",
    "concurrently": "^7.4.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.15",
    "terser": "^5.15.1",
    "tsc-watch": "^6.0.4",
    "tslint": "^5.12.1",
    "typescript": "^4.8.4"
  }
}
